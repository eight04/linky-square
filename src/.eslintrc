{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"es6": true,
		"greasemonkey": true
	},
	"globals": {
		"REPLACE": false
	},
	"rules": {
		"eqeqeq": 0,
		"camelcase": 0,
		"new-cap": 0,
		"no-constant-condition": 0,
		"no-mixed-spaces-and-tabs": 0,
		"quotes": 0,
		"no-use-before-define": [2, "nofunc"],
		"no-shadow": 0,
		"new-parens": 0,
		"global-strict": 0,
		"no-console": 0
	}
}
